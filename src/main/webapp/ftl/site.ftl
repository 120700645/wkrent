				});
				//设置订单按钮悬浮位置
				function setCityEachHide() {
					const scrollH = $(window).scrollTop();
					const documentH = $(document).height();
					if(parseFloat(scrollH / documentH) > 0.24) {
						$(".city-nav>div").hide();
					} else {
						$(".city-nav>div").show();
					}
				}
			});
